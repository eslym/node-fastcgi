{
    "name": "@eslym/fastcgi",
    "version": "1.1.0",
    "description": "FastCGI implementation for Node.js",
    "keywords": [
        "fastcgi",
        "protocol"
    ],
    "main": "dist/index.js",
    "type": "module",
    "author": "0nepeop1e",
    "license": "MIT",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/eslym/node-fastcgi.git"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "typings": "./dist/index.d.ts",
    "files": [
        "dist/index.js",
        "dist/index.d.ts"
    ],
    "scripts": {
        "build": "rimraf dist && rollup -c",
        "clean": "rimraf dist",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.1.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-typescript": "^11.1.0",
        "@types/node": "^18.16.1",
        "husky": "^8.0.3",
        "prettier": "^2.8.8",
        "pretty-quick": "^3.1.3",
        "rimraf": "^5.0.0",
        "rollup": "^3.21.0",
        "rollup-plugin-dts": "^5.3.0",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "tslib": "^2.5.0",
        "typescript": "^5.0.4"
    },
    "engines": {
        "node": ">=16.8.0"
    }
}
